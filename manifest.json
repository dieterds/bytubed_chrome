{
  "name": "BYTubeD Express",
  "version": "1.0.3",
  "homepage_url": "https://github.com/dieterds/bytubed_chrome",
  "short_name": "BYTubeDExp",
  "description": "Gets all Youtube links and generates downloadable videolinks in different resolutions.",
  "browser_action": {
    "default_icon": "gfx/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "save.html"
  },
  "icons": {
    "16": "gfx/Down-16.png",
    "24": "gfx/Down-24.png",
    "32": "gfx/Down-32.png",
    "48": "gfx/Down-48.png",
    "64": "gfx/icon.png",
    "128": "gfx/Down-128.png"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "default_locale": "en",
  "permissions": [
    "background",
    "tabs",
    "activeTab",
    "downloads",
    "storage",
    "*://*/*",
    "<all_urls>",
    "contextMenus",
    "clipboardRead",
    "clipboardWrite"
  ],
  "manifest_version": 2
}